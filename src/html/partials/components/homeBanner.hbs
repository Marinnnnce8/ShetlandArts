<div class="banner-home">
	<div data-uk-slider class="uk-position-relative">
		<ul class="uk-slider-items">
			<li class="drama">
				<div class="uk-flex uk-position-relative">
					<div data-uk-slider-parallax="x: 500,-500; easing: 2"
						class="image-box uk-background-cover uk-position-relative uk-position-z-index"
						style="background-image: url('./img/banner-home-1.png');" data-uk-img>
						<div class="uk-flex slider-buttons">
							<a class="uk-flex uk-flex-center uk-flex-middle" href="#" data-uk-slidenav-previous
								data-uk-slider-item="previous"></a>
							<a class="uk-flex uk-flex-center uk-flex-middle" href="#" data-uk-slidenav-next
								data-uk-slider-item="next"></a>
						</div>
						<ul class="uk-slider-nav uk-dotnav uk-flex-center uk-margin">
							<li uk-slider-item="0" class="uk-active"><a href=""></a></li>
							<li uk-slider-item="1"><a href=""></a></li>
							<li uk-slider-item="2"><a href=""></a></li>
						</ul>
					</div>
					<div class="uk-flex uk-flex-right uk-container uk-container-xlarge">
						<div class="uk-width-1-3@m uk-flex uk-flex-column uk-flex-center uk-position-relative"
							data-uk-slider-parallax="x: 250,-250; easing: 0.5">
							<div>
								<div class="uk-h4 uk-text-uppercase uk-margin-remove-bottom">Drama & Dance</div>
								<h1 class="uk-text-uppercase">The Swings</h1>
								<div class="uk-h6 uk-text-uppercase uk-margin-remove-top">Sat 12 Jun 2021, 1.30pm -
									2.30pm</div>
							</div>
							<div class="button-bottom">
								{{> ukButton link="#" label=cta style="secondary" size="large" icon="arrow-right"}}
							</div>
						</div>
					</div>
				</div>
			</li>
			<li class="cinema">
				<div class="uk-flex uk-position-relative">
					<div data-uk-slider-parallax="x: 500,-500; easing: 2"
						class="image-box uk-position-relative uk-position-z-index">
						<div class="uk-background-cover uk-position-relative uk-position-z-index uk-height-1-1"
							style="background-image: url('./img/banner-home-2.png');" data-uk-img
							data-uk-lightbox="animation: fade">
							<span
								class="uk-position-top-right uk-background-secondary uk-flex uk-flex-middle uk-flex-center uk-position-z-index media-icon"
								role="presentation">
								{{> renderIcon name='camera'}}
							</span>
							{{> renderIcon class="uk-position-center uk-position-z-index" name="play"}}
							<a href="https://youtu.be/jmOuxZAJifA" class="uk-position-cover" data-caption="Video"
								aria-label="Click to play media"></a>
						</div>
						<div class="uk-flex slider-buttons">
							<a class="uk-flex uk-flex-center uk-flex-middle" href="#" data-uk-slidenav-previous
								data-uk-slider-item="previous"></a>
							<a class="uk-flex uk-flex-center uk-flex-middle" href="#" data-uk-slidenav-next
								data-uk-slider-item="next"></a>
						</div>
						<ul class="uk-slider-nav uk-dotnav uk-flex-center uk-margin">
							<li uk-slider-item="0"><a href=""></a></li>
							<li class="uk-active" uk-slider-item="1"><a href=""></a></li>
							<li uk-slider-item="2"><a href=""></a></li>
						</ul>
					</div>
					<div class="uk-flex uk-flex-right uk-container uk-container-xlarge">
						<div class="uk-width-1-3@m uk-flex uk-flex-column uk-flex-center uk-position-relative"
							data-uk-slider-parallax="x: 250,-250; easing: 0.5">
							<div>
								<div class="uk-h4 uk-text-uppercase">Cinema</div>
								<h1 class="uk-text-uppercase">West Side Story 2021</h1>
								<div class="uk-h6 uk-text-uppercase">Sat 12 Jun 2021, 1.30pm - 2.30pm</div>
							</div>
							<div class="button-bottom">
								{{> ukButton link="#" label=cta style="secondary" size="large" icon="arrow-right"}}
							</div>
						</div>
					</div>
				</div>
			</li>
			<li class="music">
				<div class="uk-flex uk-position-relative">
					<div data-uk-slider-parallax="x: 500,-500; easing: 2"
						class="image-box uk-position-relative uk-position-z-index">
						<div class="uk-background-cover uk-position-relative uk-position-z-index uk-height-1-1"
							style="background-image: url('./img/banner-home-3.png');" data-uk-img
							data-uk-lightbox="animation: fade">
							<span
								class="uk-position-top-right uk-background-secondary uk-flex uk-flex-middle uk-flex-center uk-position-z-index media-icon"
								role="presentation">
								{{> renderIcon name='headphones'}}
							</span>
							<a href="https://youtu.be/jmOuxZAJifA" class="uk-position-cover" data-caption="Video"
								aria-label="Click to play media"></a>
						</div>
						<div class="uk-flex slider-buttons">
							<a class="uk-flex uk-flex-center uk-flex-middle" href="#" data-uk-slidenav-previous
								data-uk-slider-item="previous"></a>
							<a class="uk-flex uk-flex-center uk-flex-middle" href="#" data-uk-slidenav-next
								data-uk-slider-item="next"></a>
						</div>
						<ul class="uk-slider-nav uk-dotnav uk-flex-center uk-margin">
							<li uk-slider-item="0"><a href=""></a></li>
							<li uk-slider-item="1"><a href=""></a></li>
							<li class="uk-active" uk-slider-item="2"><a href=""></a></li>
						</ul>
					</div>
					<div class="uk-flex uk-flex-right uk-container uk-container-xlarge">
						<div class="uk-width-1-3@m uk-flex uk-flex-column uk-flex-center uk-position-relative"
							data-uk-slider-parallax="x: 250,-250; easing: 0.5">
							<div>
								<div class="uk-h4 uk-text-uppercase">Music</div>
								<h1 class="uk-text-uppercase">All is well in my world - Shetland Arts</h1>
								<div class="uk-h6 uk-text-uppercase">Sat 12 Jun 2021, 1.30pm - 2.30pm</div>
							</div>
							<div class="button-bottom">
								{{> ukButton link="#" label=cta style="secondary" size="large" icon="arrow-right"}}
							</div>
						</div>
					</div>
				</div>
			</li>
		</ul>
	</div>
</div>